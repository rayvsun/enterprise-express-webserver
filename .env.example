# 应用配置
NODE_ENV=development
PORT=3000
API_PREFIX=/api/v1
LOG_LEVEL=debug
ENABLE_SWAGGER=true

# JWT配置
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h

# MySQL配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=password
MYSQL_DATABASE=myapp
MYSQL_POOL_MIN=5
MYSQL_POOL_MAX=20

# Oracle配置
ORACLE_HOST=localhost
ORACLE_PORT=1521
ORACLE_USER=system
ORACLE_PASSWORD=password
ORACLE_SID=XE
ORACLE_POOL_MIN=5
ORACLE_POOL_MAX=20

# PostgreSQL配置
PG_HOST=localhost
PG_PORT=5432
PG_USER=postgres
PG_PASSWORD=password
PG_DATABASE=myapp
PG_POOL_MIN=5
PG_POOL_MAX=20

# 达梦配置
DM_HOST=localhost
DM_PORT=5236
DM_USER=SYSDBA
DM_PASSWORD=password
DM_DATABASE=myapp
DM_POOL_MIN=5
DM_POOL_MAX=20

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_PREFIX=myapp:

# 安全配置
RATE_LIMIT_WINDOW_MS=15*60*1000  # 15分钟
RATE_LIMIT_MAX=100               # 最大请求次数
CORS_ORIGIN=*                    # 允许的域名，生产环境应该指定具体域名
CORS_METHODS=GET,POST,PUT,DELETE,OPTIONS

# 集群配置
CLUSTER_ENABLED=false
CLUSTER_WORKERS=0                # 0表示使用CPU核心数

# 文件上传配置
UPLOAD_MAX_SIZE=10               # 单位MB
UPLOAD_PATH=uploads/

# 邮件配置
MAIL_HOST=smtp.example.com
MAIL_PORT=587
MAIL_USER=user@example.com
MAIL_PASSWORD=your_password
MAIL_FROM=noreply@example.com

# 外部服务API配置
# 示例：支付服务
PAYMENT_API_URL=https://api.payment.com
PAYMENT_API_KEY=your_api_key

# 性能监控配置
ENABLE_METRICS=false
METRICS_PORT=9090

# 消息队列配置
MQ_HOST=localhost
MQ_PORT=5672
MQ_USER=guest
MQ_PASSWORD=guest 