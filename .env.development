# 开发环境配置
NODE_ENV=development
PORT=3000
API_PREFIX=/api/v1
LOG_LEVEL=debug
ENABLE_SWAGGER=true

# JWT配置
JWT_SECRET=dev_jwt_secret_key_change_in_production
JWT_EXPIRES_IN=24h

# MySQL配置 - 开发环境通常使用本地数据库
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=jeecg
MYSQL_PASSWORD=********
MYSQL_DATABASE=jeecg-boot
MYSQL_POOL_MIN=2
MYSQL_POOL_MAX=10

# Redis配置 - 开发环境
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_PREFIX=webserver_dev:

# 安全配置 - 开发环境较为宽松
RATE_LIMIT_WINDOW_MS=60000        # 1分钟
RATE_LIMIT_MAX=500                # 开发环境请求限制较高
CORS_ORIGIN=*                     # 开发环境允许所有源
CORS_METHODS=GET,POST,PUT,DELETE,OPTIONS

# 集群配置 - 开发环境通常不启用集群
CLUSTER_ENABLED=false
CLUSTER_WORKERS=0

# 文件上传配置
UPLOAD_MAX_SIZE=50                # 开发环境可设置较大值
UPLOAD_PATH=uploads/dev/

# 日志配置
LOG_DIR=logs/dev

# 其他开发环境特定配置
DEBUG=express:*                   # Express调试模式
PRETTY_PRINT_LOG=true             # 美化日志输出
AUTO_RELOAD=true                  # 代码变更自动重载 