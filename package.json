{
  "name": "express-enterprise-server",
  "version": "1.0.0",
  "description": "高性能、可维护、可扩展的基于Express.js的企业级Web服务器框架",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start src/index.js -i max --name \"webserver-prod\"",
    "start:dev": "cross-env NODE_ENV=development node src/index.js",
    "start:test": "cross-env NODE_ENV=test node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch src src/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "lint": "eslint . --ext .js",
    "format": "prettier --write src/**/*.js",
    "build": "babel src -d dist",
    "build:prod": "cross-env NODE_ENV=production babel src -d dist",
    "doc": "node -e \"require('./src/config/swagger')\"",
    "debug": "cross-env NODE_ENV=development node --inspect-brk src/index.js",
    "debug:test": "cross-env NODE_ENV=test node --inspect-brk src/index.js",
    "cluster:dev": "cross-env NODE_ENV=development CLUSTER_ENABLED=true node src/index.js",
    "cluster:test": "cross-env NODE_ENV=test CLUSTER_ENABLED=true node src/index.js"
  },
  "keywords": [
    "express",
    "node.js",
    "enterprise",
    "api",
    "rest",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.3",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.2",
    "oracledb": "^6.2.0",
    "pg": "^8.11.3",
    "pm2": "^5.3.0",
    "sequelize": "^6.33.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  }
}
